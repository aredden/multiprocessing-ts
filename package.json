{
	"name": "multiprocess-ts",
	"version": "1.2.0",
	"description": "Dead simple parallel processing for node",
	"main": "index.js",
	"scripts": {
		"preversion": "npm run test",
		"pretest": "npm run build",
		"test": "mocha test/**/*.spec.js --exit",
		"bench": "node ./test/bench.js",
		"build": "babel lib -d build",
		"lint": "eslint .",
		"tt": "npx tsc && npm run pretest && npm run test"
	},
	"babel": {
		"presets": [
			"latest"
		]
	},
	"keywords": [
		"multiprocessing",
		"parallel",
		"multicore",
		"map",
		"pool"
	],
	"author": "Alex Redden",
	"license": "MIT",
	"dependencies": {
		"bluebird": "^3.7.2",
		"escope": "^3.6.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.0",
		"babel-cli": "^6.26.0",
		"babel-eslint": "^7.1.1",
		"babel-preset-latest": "^6.22.0",
		"chai": "^4.3.4",
		"chai-as-promised": "^7.1.1",
		"eslint": "^8.5.0",
		"@types/bluebird": "^3.5.36",
		"eslint-plugin-babel": "^5.3.1",
		"lodash": "^4.17.21",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	}
}
